func int add(int a, int b) {
    return a + b;
}

func int main() {
    int result = add(5, 3);
    
    for (int i = 0; i < result; i = i + 1) {
        Console.WriteLine("Count: " + i);
    }
    
    if (result > 5) {
        Console.WriteLine("Result is greater than 5");
    }
    
    return result;
}