{
  "name": "dream-syntax",
  "displayName": "Dream Syntax",
  "version": "0.0.1",
  "description": "Syntax highlighting for Dream",
  "publisher": "dream",
  "engines": {
    "vscode": "^1.90.0"
  },
  "activationEvents": [
    "onLanguage:dream"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "dream",
        "aliases": [
          "Dream",
          "dream"
        ],
        "extensions": [
          ".mlg"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./icons/Dream.svg",
          "dark": "./icons/Dream.svg"
        }
      }
    ],
    "grammars": [
      {
        "language": "dream",
        "scopeName": "source.dream",
        "path": "./syntaxes/dream.tmLanguage.json"
      }
    ]
  },
  "scripts": {
    "package": "vsce package"
  },
  "devDependencies": {
    "vsce": "^2.15.0"
  },
  "repository": ""
}
